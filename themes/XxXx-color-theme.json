	//______________________________________
	"tokenColors": [
		{
			"name": "italic",
			"scope": [
				"entity.other.inherited-class",
				"variable.language",
				"variable.language.this",
				"variable.language.super",
				"meta.var.expr",
				"constant.language",
			],
			"settings": {
				"fontStyle": "italic",
			}
		},
		{
			"name": "bold",
			"scope": [
				"support.function.",
			],
			"settings": {
				"fontStyle": "bold",
			}
		},
		{
			"name": "turquesa",
			"scope": [
				"entity.other.attribute-name.id.css",
				"storage.type.modifier.access",
				"keyword.control.new.java",
				"entity.name.type.class.java",
				"entity.name.function.js",
				"entity.name.type.class.js",
				"entity.name.function.python",
				"entity.name.type.class.python"
			],
			"settings": {
				"foreground": "#18c09e",
			}
		},
		{
			"name": "naranja",
			"scope": [
				"support.type.property-name.css",
				"variable.other.property.js",
				"entity.name.function.call.initializer.cpp",
				"variable.other.property.cpp",
				"meta.block.cpp",
				"variable.object.c",
				"variable.other.member.c",
				"meta.block.c",
				"meta.function-call.c",
				"variable.other.object.property.java",
				"variable.other.definition.java",
				"meta.function-call.generic.python",
				"meta.attribute.python",
			],
			"settings": {
				"foreground": "#FF9562",
			}
		},
		{
			"name": "rosa claro",
			"scope": [
				"support.constant.property-value.css",
				"storage.type.js",
				"variable.language",
				"storage.type.primitive",
				"storage.type.built-in.primitive",
				"storage.type.built-in.primitive.cpp",
				"storage.type.java",
				"storage.type.object.array.java",
				"support.function.builtin.python",
				"variable.other.readwrite"

			],
			"settings": {
				"foreground": "#FF5370",
			}
		},
		{
			"name": "rosa oscuro",
			"scope": [
				"storage.type.class.python",
				"storage.type.function.python",
				"storage.type.struct.c"
			],
			"settings": {
				"foreground": "#9e0c4c",
			}
		},
		{
			"name": "gris claro",
			"scope": [
				"meta.parens.cpp"
			],
			"settings": {
				"foreground": "#CBC5BE",
			}
		},
		{
			"name": "gris oscuro",
			"scope": [
				"string",
				"comment",
			],
			"settings": {
				"foreground": "#a29285",
			}
		},
		{
			"name": "azul claro",
			"scope": [
				"keyword.control.at-rule",
				"keyword.other",
				"variable.other.object.js",
				"entity.name.function.c",
				"meta.body.function.definition.cpp",
				"meta.body.class.cpp",
				"variable.other.object.java",
				"support.function.builtin.python",
				"variable.other.readwrite.js"

			],
			"settings": {
				"foreground": "#18a1c0",
			}
		},
		{
			"name": "azul oscuro",
			"scope": [
				"constant.numeric",
			],
			"settings": {
				"foreground": "#3f7eb3",
			}
		},
		{
			"name": "verde claro",
			"scope": [
				"keyword.operator",
				"punctuation.separator",
			],
			"settings": {
				"foreground": "#85B56D",
			}
		},
		{
			"name": "verde oscuro",
			"scope": [
				"constant.language",
			],
			"settings": {
				"foreground": "#13990e",
			}
		},
		{
			"name": "morado claro",
			"scope": [
				"entity.other.attribute-name",
				"keyword.control.flow",
				"keyword.control.java",
				"keyword.control.while.cpp",
				"keyword.control.for.cpp",
				"keyword.control.c",
				"keyword.control.return",
				"keyword.control.try.cpp",
				"keyword.control.try.java",
				"keyword.control.catch.java",
				"keyword.control.loop.js",
				"keyword.control.trycatch.js"
			],
			"settings": {
				"foreground": "#C285C0",
				"fontStyle": "bold",
			}
		},
		{
			"name": "morado oscuro",
			"scope": [
				"entity.name.function.constructor.cpp",
				"meta.head.function.definition.cpp",
				"variable.other.object.access.c",
				"entity.name.function.java",
				"keyword.other.import.java",
				"support.type.python",


			],
			"settings": {
				"foreground": "#c9118f",
			}
		},
		{
			"name": "turquesa claro",
			"scope": [
				"storage.modifier.java",
				"storage.type.function",
				"storage.type.class",
				"keyword.control.import",
				"keyword.control.directive",
				"entity.name.function.member.cpp",
				"entity.name.function.call.cpp",

			],
			"settings": {
				"foreground": "#2f978d",
			}
		},
		{
			"name": "moramora",
			"scope": [
				"string.quoted.other",
				"entity.name.tag",
				"support.function.magic.python",
			],
			"settings": {
				"foreground": "#9e990c",
			}
		},
		{
			"name": "Amarillo",
			"scope": [
				"entity.name.scope-resolution.parameter.cpp",
				"entity.name.scope-resolution.cpp",
			],
			"settings": {
				"foreground": "#bdcc4b",
			}
		}
	],
	"semanticHighlighting": true,
	"semanticTokenColors": {
		"namespace": "#bdcc4b",
		"builtinConstant": "#13990e",
		"module": "#7700ff",
		"parameter": {
			"fontStyle": "italic"
		},
	},
	"editor.cursorBlinking": "smooth",
	"editor.cursorSmoothCaretAnimation": "on",
	"window.autoDetectColorScheme": true,

